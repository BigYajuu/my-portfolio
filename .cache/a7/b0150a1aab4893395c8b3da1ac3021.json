ce":"..\\css\\custom_style.scss","name":null,"original":{"line":299,"column":0},"generated":{"line":1,"column":3568}},{"source":"..\\css\\custom_style.scss","name":null,"original":{"line":299,"column":0},"generated":{"line":1,"column":3582}},{"source":"..\\css\\custom_style.scss","name":null,"original":{"line":300,"column":4},"generated":{"line":1,"column":3592}},{"source":"..\\css\\custom_style.scss","name":null,"original":{"line":300,"column":4},"generated":{"line":1,"column":3593}},{"source":"..\\css\\custom_style.scss","name":null,"original":{"line":301,"column":8},"generated":{"line":1,"column":3607}},{"source":"..\\css\\custom_style.scss","name":null,"original":{"line":301,"column":8},"generated":{"line":1,"column":3608}}],"sources":{"..\\css\\custom_style.scss":"/* Constants */\n/* Display and Page Margins */\n$viewport-min-width: 1000px;\n$page-margin-desktop: 8.33%;\n$page-margin-mobile: 2%;\n/* Elemental Paddings and Curves*/\n$element-offset-left: 24px;\n$border-radius-standard: 24px;\n$container-margin: 8px;\n$container-padding: 16px;\n$transition-time: 0.6s;\n$transition-time-debug: 3s;\n\n* {\n    box-sizing: border-box;\n}\n\nbody {\n    font-family: 'BauPro', Helvetica, Arial, sans-serif;\n    font-weight: normal;\n    font-style: normal;\n    overscroll-behavior-x: none;\n    position: relative;\n}\n\n.main-scaffold{\n    height: 100vh;\n    overflow-y: auto;\n}\n\n.header {\n    width: 100%;\n    box-shadow: 0 0 1rem 0 rgba(0, 0, 0, 0.2);\n    background-color: rgba(255, 255, 255, 0.75);\n    backdrop-filter: blur(5px);\n    display: block;\n    padding-top: 16px;\n    padding-bottom: 12px;\n    padding-left: calc($page-margin-mobile + $element-offset-left);\n    padding-right: calc($page-margin-mobile + $element-offset-left);\n    @media (min-width: $viewport-min-width) {\n        padding-left: calc($page-margin-desktop + $element-offset-left);\n        padding-right: calc($page-margin-desktop + $element-offset-left);\n    }\n}\n\n.container {\n    width: 100%;\n    box-shadow: 0 0 0.5rem 0 rgba(0, 0, 0, 0.2);\n    border-radius: $border-radius-standard;\n    background: url('../img/noise.svg');\n    background-blend-mode: soft-light;\n    background-color: rgba(255, 255, 255, 0.15);\n    backdrop-filter: blur(5px);\n    display: inline-block;\n    padding: $container-padding;\n    margin-top: $container-margin;    // For vertical margin only\n    margin-bottom: $container-margin;\n    overflow-wrap: normal;\n}\n\n.col-default-padding {\n    // For horizontal padding only\n    padding-left: $container-margin;\n    padding-right: $container-margin;\n}\n\n.section{\n    width: 100%;\n    &-default-page-margin {\n        margin-left: $page-margin-mobile;    // For side margins of the entire page\n        margin-right: $page-margin-mobile;\n        @media (min-width: $viewport-min-width) {\n            margin-left: $page-margin-desktop;\n            margin-right: $page-margin-desktop;\n        }\n    }\n}\n\n.section-text{\n    padding-left: $element-offset-left;\n}\n\n.row:after{\n    content: \" \";\n    clear: both;\n    display: table;\n}\n\n/* Text Styles and Paragraphs */\n/* Set All Text Margin to Zero as Default */\nh1 {\n    margin: 0;\n}\n\np {\n    margin: 0;\n}\n\n.paragraph-margin {\n    margin-top: 24px;\n}\n\n.section-margin {\n    margin-top: 48px;\n    margin-bottom: 12px;\n}\n\n/* Background Colour Schemes */\n.entire-window {\n    height: 100vh;\n    width: 100%;\n    overflow-y: auto;\n}\n\n.dynamic-bg {\n    background: \n        linear-gradient(to right, #ff7e00, #ebff10),\n        linear-gradient(to top, #ff0042, #f72ddd);\n    background-blend-mode: lighten;\n    background-size: cover;\n    background-attachment: fixed;\n}\n.dynamic-bg:before dynamic-bg:after {\n    display: block;\n    content: \"\";\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n}\n\n.scroll-chevron {\n    &-left {\n        position: fixed;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        /* Height is detected by JQuery's outerHeight() */\n        width: 0%;\n        opacity: 0;\n        z-index: 1;\n        left: 0;\n        background-image: linear-gradient(to right, rgba(0, 0, 0, 0.70) 25%, rgba(0, 0, 0, 0) 100%);\n    }\n    &-right {\n        position: fixed;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        /* Height is detected by JQuery's outerHeight() */\n        width: 0%;\n        opacity: 0;\n        z-index: 1;\n        right: 0;\n        background-image: linear-gradient(to left, rgba(0, 0, 0, 0.70) 25%, rgba(0, 0, 0, 0) 100%);\n    }\n}\n\n/* 3D Transitions */\n.scene{\n    perspective: 400vh;\n    position: relative;\n}\n\n.scene-removed{\n    display: none;\n}\n\n#pan {\n    transform-style: preserve-3d;\n}\n  \n#cube {\n    width: 100%;\n    height: 100%;\n    position: absolute;\n    transform-style: preserve-3d;\n}\n\n.panScrollUp {\n    animation: moveCubeScrollUp $transition-time ease;\n}\n\n.panScrollDown {\n    animation: moveCubeScrollDown $transition-time ease;\n}\n\n.rotateCubeScrollUp {\n    animation: rotateCubeScrollUp $transition-time ease;\n}\n\n.rotateCubeScrollDown {\n    animation: rotateCubeScrollDown $transition-time ease;\n}\n\n@keyframes moveCubeScrollUp {\n    0% { transform: translateZ(0); }\n    100% { transform: translateY(100vh) translateZ(100vh); }\n}\n\n@keyframes moveCubeScrollDown {\n    0% { transform: translateZ(0); }\n    100% { transform: translateZ(-100vh); }\n}\n\n@keyframes rotateCubeScrollUp {\n    0% { transform: translateY(-100vh) rotateX(0deg) rotateY(0deg) rotateZ(0deg); }\n    100% { transform: rotateX(-90deg) rotateY(0deg) rotateZ(0deg); }\n}\n\n@keyframes rotateCubeScrollDown {\n    0% { transform: rotateX(0deg) rotateY(0deg) rotateZ(0deg); }\n    100% { transform: rotateX(90deg) rotateY(0deg) rotateZ(0deg); }\n}\n\n#cube .front  {\n    transform: rotateY(0deg) translateZ(0);\n}\n#cube .top    {\n    transform: rotateX(90deg) translateZ(-50vh) translateY(-50vh);\n}\n#cube .bottom {\n    transform: rotateX(-90deg) translateZ(-50vh) translateY(50vh);\n}\n\n/* Responsive Col */\n.col-1 {\n    width: 8.33%;\n}\n.col-2 {\n    width: 16.66%;\n}\n.col-3 {\n    width: 25%;\n}\n.col-4 {\n    width: 33.33%;\n}\n.col-5 {\n    width: 41.66%;\n}\n.col-6 {\n    width: 50%;\n}\n.col-7 {\n    width: 58.33%;\n}\n.col-8 {\n    width: 66.66%;\n}\n.col-9 {\n    width: 75%;\n}\n.col-10 {\n    width: 83.33%;\n}\n.col-11 {\n    width: 91.66%;\n}\n.col-12 {\n    width: 100%;\n}\n.col-abs-width {\n    width: 400px;\n}\n.col-page-margin {\n    width: $page-margin-mobile;\n    @media (min-width: $viewport-min-width) {\n        width: $page-margin-desktop;\n    }\n}\n\n[class*=\"col-\"] {\n    float: left;\n}\n\n.x-scrollable [class*=\"col-\"] {\n    float: none;\n    display: inline-flex;\n}\n\n.x-scrollable {\n    position: relative;\n    overflow-x: auto;\n    white-space: nowrap;\n    padding-left: $page-margin-desktop;\n    @media (max-width: $viewport-min-width) {\n        padding-left: $page-margin-mobile;\n    }\n}\n\n.float-right {\n    float: right;\n}\n\n/* Collapsible Measures */\n@media (max-width: $viewport-min-width) {\n    .row-covering {\n        width: 100%;\n        flex-wrap: wrap;\n    }\n}\n\n"},"lineCount":null}},"error":null,"hash":"d20b0a009728b3acfc7aabeae27d1031","cacheData":{"env":{}}}